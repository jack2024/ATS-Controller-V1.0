/* ATM90E36 Energy Monitor Functions
*/
#include "D:\STM32\STM32F070CBT6 Project\Pjt2 - Copy - Copy\TestSPI_MCP23S17\MDK-ARM\M90E26.h"

void Delay_usec(uint32_t count)
{
	for(uint8_t i=0; i<count; i++)
  {
		for(uint8_t j=0; j<20; j++)
		{
			__ASM volatile ("NOP");
			__ASM volatile ("NOP");
			__ASM volatile ("NOP");
			__ASM volatile ("NOP");
			__ASM volatile ("NOP");
			__ASM volatile ("NOP");
			__ASM volatile ("NOP");
			__ASM volatile ("NOP");
			__ASM volatile ("NOP");
		}
	}
}


